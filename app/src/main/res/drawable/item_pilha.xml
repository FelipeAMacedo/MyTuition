<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:bottom="0sp"
        android:left="-2sp"
        android:right="-2sp"
        android:top="-2sp">
        <shape android:shape="rectangle" >
            <solid android:color="#ffffff" />

            <stroke
                android:width="1sp"
                android:color="#d6d6d6" />
        </shape>
    </item>
</layer-list>
